<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ratingApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ratingApp"
    Shell.FlyoutBehavior="Disabled"
    Title="ratingApp">

    <TabBar>
        <ShellContent Title="Home" ContentTemplate="{DataTemplate local:MainPage}" />
        <ShellContent Title="Explore" ContentTemplate="{DataTemplate local:ExplorePage}" />
        <ShellContent Title="Rate" ContentTemplate="{DataTemplate local:RatingPage}" />
        <ShellContent Title="Bookmarks" ContentTemplate="{DataTemplate local:BookmarkPage}" />
        <ShellContent Title="Settings" ContentTemplate="{DataTemplate local:SettingsPage}" />
        
    </TabBar>



</Shell>
